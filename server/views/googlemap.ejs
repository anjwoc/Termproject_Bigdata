<!DOCTYPE html>
<html>
<head>
    <style>
        #map {
            height: 400px;
            width: 100%;
            flaot: top;
        }
    </style>
</head>
<body>
<div id="map"></div>
<script>
    function initMap() {
        var uluru = {lat: 37.250943, lng: 127.028344};
        var map = new google.maps.Map(document.getElementById('map'), {
            zoom: 4,
            center: uluru
        });
        var marker = new google.maps.Marker({
            position: uluru,
            map: map
        });
    }
</script>
<script async defer
        src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDRn7Pyp1Q3tgqBxXt3jiGWssL1WriauYs&callback=initMap">
</script>
</body>
</html>